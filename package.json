  "dependencies": {
    "chalk": "^5.0.1",
    "configstore": "^6.0.0",
    "debug": "^4.3.4",
    "desm": "^1.3.0",
    "extract-zip": "^2.0.1",
    "fetch-pkg": "^1.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "latest-version": "^7.0.0",
    "merge-options": "^3.0.4",
    "ora": "^6.1.0",
